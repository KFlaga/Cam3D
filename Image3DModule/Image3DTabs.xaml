<UserControl x:Class="Image3DModule.Image3DTabs"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Image3DModule"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <TabControl>
        <TabItem Header="Cube Points">
            <local:Image3DFromPointsTab x:Name="_tabFromPoints"/>
        </TabItem>
        <TabItem Header="Disparity Segmented">
            <local:Image3DWithDisparityTab x:Name="_tabWithDisparity"/>
        </TabItem>
    </TabControl>
</UserControl>
